<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>ReportesMonitoreo-Web</display-name>
	
	<servlet>
		<servlet-name>consultarReporteServlet</servlet-name>
		<servlet-class>co.gov.ideamredd.servlets.ConsultarReporteServlet</servlet-class>
	</servlet> 
	<servlet-mapping>
		<servlet-name>consultarReporteServlet</servlet-name>
		<url-pattern>/consultarReporteServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>descargaDocumentosServlet</servlet-name>
		<servlet-class>co.gov.ideamredd.servlets.DescargaDocumentosServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>descargaDocumentosServlet</servlet-name>
		<url-pattern>/descargaDocumentosServlet</url-pattern> 
	</servlet-mapping>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>co.gov.ideamredd.servlets.IdiomaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/idiomaServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
