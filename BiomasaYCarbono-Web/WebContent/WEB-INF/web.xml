<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BiomasaYCarbono-Web</display-name>
  <servlet>
    <servlet-name>RegistrarBiomasaServlet</servlet-name>
    <servlet-class>co.gov.ideamredd.servlets.RegistrarBiomasaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CambiarEstadoBiomasaServlet</servlet-name>
    <servlet-class>co.gov.ideamredd.servlets.CambiarEstadoBiomasaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RedireccionBiomasaServlet</servlet-name>
    <servlet-class>co.gov.ideamredd.servlets.RedireccionBiomasaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrarBiomasaServlet</servlet-name>
    <url-pattern>/registrarbiomasa</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CambiarEstadoBiomasaServlet</servlet-name>
    <url-pattern>/estadobiomasa</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RedireccionBiomasaServlet</servlet-name>
    <url-pattern>/redireccion</url-pattern>
  </servlet-mapping>
</web-app>